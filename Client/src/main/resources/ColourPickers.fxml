<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="294.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controllers.ColourPickersController">
   <children>
      <VBox prefHeight="200.0" prefWidth="100.0" spacing="10.0" AnchorPane.bottomAnchor="15.0" AnchorPane.leftAnchor="5.0" AnchorPane.rightAnchor="5.0" AnchorPane.topAnchor="15.0">
         <children>
            <Label alignment="CENTER" contentDisplay="CENTER" prefHeight="20.0" prefWidth="302.0" text="%visual.pick">
               <font>
                  <Font size="14.0" />
               </font>
            </Label>
            <ScrollPane hbarPolicy="NEVER" prefHeight="348.0" prefWidth="284.0" VBox.vgrow="ALWAYS">
               <content>
                  <VBox fx:id="pickersVbox" prefHeight="303.0" prefWidth="283.0" spacing="5.0" />
               </content></ScrollPane>
            <HBox alignment="CENTER" prefHeight="27.0" prefWidth="284.0" spacing="15.0">
               <children>
                  <Button fx:id="applyBtn" contentDisplay="CENTER" mnemonicParsing="false" text="%alerts.apply" />
                  <Button fx:id="cancelBtn" mnemonicParsing="false" text="%alerts.cancel" />
               </children>
            </HBox>
         </children>
      </VBox>
   </children>
</AnchorPane>
